{
    "name": "Remove Google Redirect in Google Results",
    "short_name": "DeGoogle",
    "description": "Disables the JS function that adds redirect to Google Search Results.",
    "version": "0.0.1",
    "permissions": [
        "tabs"
    ],
    "page_action": {
        "default_title": "DeGoogle"
    },
    "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["content.js"],
        "run_at": "document_end"
        }
    ],
    "manifest_version" : 2,
    "icons": {"32": "icon32.png"}
}
